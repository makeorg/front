{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "Actions/*": [
        "src/actions/*"
      ],
      "Api/*": [
        "src/api/*"
      ],
      "Assets/*": [
        "src/assets/*"
      ],
      "Components/*": [
        "src/components/*"
      ],
      "Containers/*": [
        "src/containers/*"
      ],
      "Constants/*": [
        "src/constants/*"
      ],
      "Helpers/*": [
        "src/helpers/*"
      ],
      "Pages/*": [
        "src/pages/*"
      ],
      "Services/*": [
        "src/services/*"
      ],
      "Types/*": [
        "src/types/*"
      ],
      "Shared/*": [
        "shared/*"
      ]
    },
  },
  "exclude": [
    "node_modules"
  ]
}