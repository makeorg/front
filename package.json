	"scripts": {
		"dev": "webpack --mode development --progress --color --config ./webpack/config.babel.js && node bin/versionBuild.js && node -r dotenv/config bin/start dotenv_config_path=.env.local",
		"build": "webpack --mode production --progress --color --config ./webpack/config.babel.js && node bin/versionBuild.js",
		"server": "pm2-runtime start ecosystem.config.js",
		"lint": "eslint client server shared",
		"flow": "flow",
		"jscpd": "jscpd client server shared",
		"translation": "npx babel --config-file ./babel.config.js i18n --out-dir dist/i18n i18n && bin/checkTranslation.js check -d ./server/staticData/i18n -l 'fr'",
		"documentation": "bin/checkDocumentation.js check",
		"accessibility": "bin/checkAccessibility.js check",
		"test": "jest ./client/.*.spec.js ./server/.*.spec.js ./shared/.*.spec.js",
		"ci:server": "node ./apiMocks/server.js & node -r dotenv/config bin/start dotenv_config_path=.env.ci",
		"ci": "env NODE_ENV=ci start-server-and-test ci:server http://localhost:9009",
		"cypress:run": "cypress run -b chrome --headless",
		"cypress:open": "cypress open",
		"lighthouse": "bin/lighthouse.js",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
