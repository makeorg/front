	"scripts": {
		"build": "webpack -p --progress --colors --config ./webpack/prod.config.js",
		"start:dev": "env HOST=local.makeorg.tech  webpack-dev-server --progress --colors --config ./webpack/dev.config.js --hot",
		"server:dev": "yarn server:dev:api-mock:start & yarn server:dev:server:start",
		"server:preprod": "env NODE_ENV=development PORT=443 API_URL=https://local.makeorg.tech/api-local PROXY_TARGET_API_URL=https://api.preprod.makeorg.tech FRONT_URL=https://local.makeorg.tech node bin/start",
		"server": "env PORT=9009 pm2-runtime start ecosystem.config.js",
		"server:dev:server:start": "env PORT=9009 NODE_ENV=development node bin/start",
		"server:dev:api-mock:start": "node ./apiMocks/server.js",
		"version:build": "node server/versionBuild.js",
		"prepush": "yarn lint && yarn flow && yarn test:unit:all && yarn jscpd",
		"styleguide": "styleguidist server",
		"styleguide:build": "styleguidist build",
		"lint": "eslint client server shared",
		"test:server:unit": "jest ./server/.*.spec.js",
		"test:server:unit:watch": "npm run test:server:unit -- --watch",
		"test:shared:unit": "jest ./shared/.*.spec.js",
		"test:shared:unit:watch": "npm run test:shared:unit -- --watch",
		"test:unit": "jest ./client/.*.spec.js",
		"test:unit:watch": "npm run test:unit -- --watch",
		"test:unit:all": "jest ./client/.*.spec.js ./server/.*.spec.js ./shared/.*.spec.js",
		"test:unit:all:watch": "npm run test:unit:all -- --watch",
		"test-with-coverage": "npm run test:unit:all -- --coverage",
		"test-with-coverage-codacy": "npm run test-with-coverage && cat ./coverage/lcov.info | codacy-coverage --token=$CODACY_TOKEN",
		"flow": "flow",
		"test:cypress": "env NODE_ENV=test start-server-and-test server:dev http://localhost:9009 cypress:run",
		"cypress:run": "cypress run -b chrome --headless",
		"cypress:open": "cypress open",
		"lighthouse:run": "env NODE_ENV=test start-server-and-test server:dev http://localhost:9009 test:lighthouse:all",
		"test:lighthouse:all": "yarn test:lighthouse:home && yarn test:lighthouse:sequence && yarn test:lighthouse:consultation && yarn test:lighthouse:proposal",
		"test:lighthouse:home": "lighthouse-ci http://localhost:9009 --accessibility=100 --report='.' --filename=report-home.html --chrome-flags='--headless --no-sandbox'",
		"test:lighthouse:sequence": "lighthouse-ci http://localhost:9009/FR-fr/consultation/question-0-slug/selection --accessibility=100 --report='.' --filename=report-sequence.html --chrome-flags='--headless --no-sandbox'",
		"test:lighthouse:consultation": "lighthouse-ci http://localhost:9009/FR-fr/consultation/question-0-slug/consultation --accessibility=100 --report='.' --filename=report-consultation.html --chrome-flags='--headless --no-sandbox'",
		"test:lighthouse:proposal": "lighthouse-ci http://localhost:9009/FR-fr/consultation/question-0-slug/proposal/proposal-question-0-slug-0-id/proposal-question-0-slug-0-slug --accessibility=100 --report='.' --filename=report-proposal.html --chrome-flags='--headless --no-sandbox'",
		"svg": "svgr -d ./client/ui/Svg/elements ./client/ui/Svg/svgr --template ./client/ui/Svg/svgrTemplate.js && cp -r ./client/ui/Svg/svgr/* ./client/ui/Svg/source",
		"jscpd": "jscpd client server shared"
	}
