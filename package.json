	"scripts": {
		"start:dev": "webpack-dev-server --progress --colors --config ./webpack/dev.config.js --hot",
		"build": "webpack -p --progress --colors --config ./webpack/prod.config.js",
		"server:dev": "env NODE_ENV=development PORT=9009 node bin/start",
		"server": "env PORT=9009 pm2-runtime start ecosystem.config.js",
		"version:build": "node server/versionBuild.js",
		"prepush": "yarn lint && yarn flow && yarn test:unit:all",
		"styleguide": "styleguidist server",
		"styleguide:build": "styleguidist build",
		"lint": "eslint client server shared",
		"api-mock:start": "node ./apiMocks/server.js",
		"test:server:unit": "jest ./server/.*.spec.js",
		"test:server:unit:watch": "npm run test:server:unit -- --watch",
		"test:shared:unit": "jest ./shared/.*.spec.js",
		"test:shared:unit:watch": "npm run test:shared:unit -- --watch",
		"test:unit": "jest ./client/.*.spec.js",
		"test:unit:watch": "npm run test:unit -- --watch",
		"test:unit:all": "jest ./client/.*.spec.js ./server/.*.spec.js ./shared/.*.spec.js",
		"test:unit:all:watch": "npm run test:unit:all -- --watch",
		"test-with-coverage": "npm run test:unit:all -- --coverage",
		"test-with-coverage-codacy": "npm run test-with-coverage && cat ./coverage/lcov.info | codacy-coverage --token=$CODACY_TOKEN",
		"flow": "flow",
		"test:cypress": "start-server-and-test start:dev http://localhost:3000 cypress:run",
		"cypress:run": "cypress run",
		"cypress:open": "cypress open",
		"svg": "svgr -d ./client/ui/Svg/elements ./client/ui/Svg/svgr --template ./client/ui/Svg/svgrTemplate.js && cp -r ./client/ui/Svg/svgr/* ./client/ui/Svg/source"
	}
